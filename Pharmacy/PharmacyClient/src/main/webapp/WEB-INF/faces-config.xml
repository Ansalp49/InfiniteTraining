<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">
	<!-- This file must be present with our jar, even if it is empty. Otherwise, 
		our Java annotations won't get scanned! -->
	<managed-bean>
		<managed-bean-name>mainImpl</managed-bean-name>
		<managed-bean-class>com.java.ejb.PharmaLoginImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pharmacyLogin</managed-bean-name>
		<managed-bean-class>com.java.ejb.PharmacyLogin</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pharImpl</managed-bean-name>
		<managed-bean-class>com.java.ejb.PharmacyImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mainshow</managed-bean-name>
		<managed-bean-class>com.java.ejb.MainImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>res</managed-bean-name>
		<managed-bean-class>com.java.ejb.Prescription</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>prov</managed-bean-name>
		<managed-bean-class>com.java.ejb.ProviderDetails</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>receipt</managed-bean-name>
		<managed-bean-class>com.java.ejb.RecipientDetails</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>entry</managed-bean-name>
		<managed-bean-class>com.java.ejb.MedicineEntry</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sales</managed-bean-name>
		<managed-bean-class>com.java.ejb.MedicineSales</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>paginationDao</managed-bean-name>
		<managed-bean-class>com.java.ejb.Pagination</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pagebean</managed-bean-name>
		<managed-bean-class>com.java.ejb.JsfPagination
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pageDao</managed-bean-name>
		<managed-bean-class>com.java.ejb.JsfPaginationBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pageimpl</managed-bean-name>
		<managed-bean-class>com.java.ejb.JsfPaginationBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>presImpl</managed-bean-name>
		<managed-bean-class>com.java.ejb.PrescriptionDAOImpl
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>impl</managed-bean-name>
		<managed-bean-class>com.java.ejb.PharmacyImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>entries</managed-bean-name>
		<managed-bean-class>com.java.ejb.MedicineEntry</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sale</managed-bean-name>
		<managed-bean-class>com.java.ejb.MedicineSales</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>controller</managed-bean-name>
		<managed-bean-class>com.java.ejb.ControllerEjb</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>entries</property-name>
			<property-class>com.java.ejb.MedicineEntry</property-class>
			<value>#{entries}</value>
		</managed-property>
		<managed-property>
			<property-name>impl</property-name>
			<property-class>com.java.ejb.PharmacyImpl</property-class>
			<value>#{impl}</value>
		</managed-property>
		<managed-property>
			<property-name>sale</property-name>
			<property-class>com.java.ejb.MedicineSales</property-class>
			<value>#{sale}</value>
		</managed-property>
		<managed-property>
			<property-name>pageimpl</property-name>
			<property-class>com.java.ejb.JsfPaginationBean</property-class>
			<value>#{pageimpl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>medController</managed-bean-name>
		<managed-bean-class>com.java.ejb.MedicinesController
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
</faces-config>